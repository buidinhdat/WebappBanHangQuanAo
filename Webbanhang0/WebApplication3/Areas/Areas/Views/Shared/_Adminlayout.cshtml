    <!-- container section start -->
    <!-- javascripts -->
    <script src="~/cssadmin/js/jquery.js"></script>
    <script src="~/cssadmin/js/jquery-ui-1.10.4.min.js"></script>
    <script src="~/cssadmin/js/jquery-1.8.3.min.js"></script>
    <script type="~/cssadmin/text/javascript" src="js/jquery-ui-1.9.2.custom.min.js"></script>
    <!-- bootstrap -->
    <script src="~/cssadmin/js/bootstrap.min.js"></script>
    <!-- nice scroll -->
    <script src="~/cssadmin/js/jquery.scrollTo.min.js"></script>
    <script src="~/cssadmin/js/jquery.nicescroll.js" type="text/javascript"></script>
    <!-- charts scripts -->
    <script src="~/cssadmin/assets/jquery-knob/js/jquery.knob.js"></script>
    <script src="~/cssadmin/js/jquery.sparkline.js" type="text/javascript"></script>
    <script src="~/cssadmin/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.js"></script>
    <script src="~/cssadmin/js/owl.carousel.js"></script>
    <!-- jQuery full calendar -->
    <
    <script src="~/cssadmin/js/fullcalendar.min.js"></script>
    <!-- Full Google Calendar - Calendar -->
    <script src="~/cssadmin/assets/fullcalendar/fullcalendar/fullcalendar.js"></script>
    <!--script for this page only-->
    <script src="~/cssadmin/js/calendar-custom.js"></script>
    <script src="~/cssadmin/js/jquery.rateit.min.js"></script>
    <!-- custom select -->
    <script src="~/cssadmin/js/jquery.customSelect.min.js"></script>
    <script src="~/cssadmin/assets/chart-master/Chart.js"></script>

    <!--custome script for all page-->
    <script src="~/cssadmin/js/scripts.js"></script>
    <!-- custom script for this page-->
    <script src="~/cssadmin/js/sparkline-chart.js"></script>
    <script src="~/cssadmin/js/easy-pie-chart.js"></script>
    <script src="~/cssadmin/js/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/cssadmin/js/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/cssadmin/js/xcharts.min.js"></script>
    <script src="~/cssadmin/js/jquery.autosize.min.js"></script>
    <script src="~/cssadmin/js/jquery.placeholder.min.js"></script>
    <script src="~/cssadmin/js/gdp-data.js"></script>
    <script src="~/cssadmin/js/morris.min.js"></script>
    <script src="~/cssadmin/js/sparklines.js"></script>
    <script src="~/cssadmin/js/charts.js"></script>
    <script src="~/cssadmin/js/jquery.slimscroll.min.js"></script>
    <script>
        //knob
        $(function () {
            $(".knob").knob({
                'draw': function () {
                    $(this.i).val(this.cv + '%')
                }
            })
        });

        //carousel
        $(document).ready(function () {
            $("#owl-slider").owlCarousel({
                navigation: true,
                slideSpeed: 300,
                paginationSpeed: 400,
                singleItem: true

            });
        });

        //custom select box

        $(function () {
            $('select.styled').customSelect();
        });

        /* ---------- Map ---------- */
        $(function () {
            $('#map').vectorMap({
                map: 'world_mill_en',
                series: {
                    regions: [{
                        values: gdpData,
                        scale: ['#000', '#000'],
                        normalizeFunction: 'polynomial'
                    }]
                },
                backgroundColor: '#eef3f7',
                onLabelShow: function (e, el, code) {
                    el.html(el.html() + ' (GDP - ' + gdpData[code] + ')');
                }
            });
        });
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>

